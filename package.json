{
  "scripts": {
    "prepare": "husky install",
    "pre-commit": "run-p 'pre-commit:*'",
    "pre-commit:format": "prettier --write --ignore-unknown .",
    "pre-commit:lint": "eslint",
    "pre-commit:typecheck": "tsc --noEmit",
    "dev": "EXPO_USE_PATH_ALIASES=1 expo start -c",
    "android": "EXPO_USE_PATH_ALIASES=1 expo start --android",
    "ios": "EXPO_USE_PATH_ALIASES=1 expo start --ios",
    "web": "EXPO_USE_PATH_ALIASES=1 expo start --web",
    "update:packages": "ncu -u -x 'react-native-reanimated,react-native-svg' && yarn",
    "build:development:ios": "eas build --platform ios --profile development",
    "build:development:android": "eas build --platform android --profile development",
    "build:preview:ios": "eas build --platform ios --profile preview",
    "build:preview:android": "eas build --platform android --profile preview",
    "build:production:ios": "eas build --platform ios --profile production",
    "build:production:android": "eas build --platform android --profile production",
    "build:production:web": "expo export:web",
    "deploy:web": "yarn build:production:web && npx firebase deploy",
    "check:web": "npx serve web-build",
    "start": "EXPO_USE_PATH_ALIASES=1 expo start -c"
  },
  "dependencies": {
    "@expo-google-fonts/noto-sans-jp": "0.2.3",
    "@expo-google-fonts/outfit": "0.2.3",
    "@react-native-async-storage/async-storage": "1.17.11",
    "@react-native-community/cli-platform-android": "10.2.0",
    "@react-native-community/cli-platform-ios": "10.2.0",
    "@react-navigation/native": "6.1.6",
    "@react-navigation/native-stack": "6.9.12",
    "@types/react": "18.0.28",
    "clsx": "1.2.1",
    "expo": "48.0.5",
    "expo-checkbox": "2.3.1",
    "expo-font": "11.1.1",
    "i18next": "22.4.10",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "12.2.0",
    "react-native": "0.71.3",
    "react-native-gesture-handler": "2.9.0",
    "react-native-heroicons": "3.2.0",
    "react-native-popup-menu": "0.16.1",
    "react-native-reanimated": "2.14.4",
    "react-native-recoil-persist": "0.0.6",
    "react-native-safe-area-context": "4.5.0",
    "react-native-screens": "3.20.0",
    "react-native-svg": "13.4.0",
    "react-native-svg-transformer": "1.0.0",
    "react-native-toast-message": "2.1.6",
    "react-native-utilities": "0.1.7",
    "react-native-web": "0.18.12",
    "recoil": "0.7.7",
    "twrnc": "3.6.0"
  },
  "devDependencies": {
    "@babel/core": "7.21.0",
    "@expo/metro-config": "0.7.1",
    "@expo/webpack-config": "18.0.1",
    "@svgr/webpack": "6.5.1",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "5.54.0",
    "@typescript-eslint/parser": "5.54.0",
    "babel-plugin-module-resolver": "5.0.0",
    "eas-cli": "3.7.2",
    "eslint": "8.35.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-native": "4.0.0",
    "husky": "8.0.3",
    "npm-check-updates": "16.7.10",
    "prettier": "2.8.4",
    "prettier-plugin-tailwindcss": "0.2.4",
    "typescript": "4.9.5"
